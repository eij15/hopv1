{
  "name": "hopv1",
  "version": "0.1.0",
  "description": "Verkefnið felst í því að smíða vef eftir forskrift.",
  "main": "index.js",
  "dependencies": {
    "node-sass": "^4.9.4"
  },
  "devDependencies": {
    "browser-sync": "^2.26.3",
    "npm-run-all": "^4.1.3"
  },
  "scripts": {
    "browser-sync": "browser-sync start --server --files index.html styles.css",
    "sass": "node-sass --source-map-embed styles.scss style.css",
    "sass-watch": "node-sass --source-map-embed --watch styles.scss --recursive styles.css",
    "dev": "npm-run-all --parallel sass sass-watch browser-sync"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eij15/hopv1.git"
  },
  "keywords": [],
  "author": "Einar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/eij15/hopv1/issues"
  },
  "homepage": "https://github.com/eij15/hopv1#readme"
}
